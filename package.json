{
	"name": "cutted-server",
	"version": "1.0.0",
	"description": "",
	"types": "types/index",
	"main": "dest/index",
	"scripts": {
		"_preinstall": "cd __ecosystem-utils && node _preinstall.js && cd ..",
		"preinstall": "npm run _preinstall",
		"_postinstall": "cd __ecosystem-utils && node _postinstall.js && cd ..",
		"postinstall": "npm run build:min && npm run _postinstall",
		"start": "node dest/index.js",
		"start:ts": "ts-node src/index.ts",
		"start:ts:debug": "node --inspect-brk=9229 --nolazy -r ts-node/register src/index.ts",
		"std": "npm run start:ts:debug",
		"minify": "cd __ecosystem-utils && node __minifier.js && cd ..",
		"build": "tsc -p .",
		"build:start": "npm run build && npm run start",
		"build:live": "nodemon --watch \"src/**/*.*\" --exec \"npm run std\"",
		"build:min": "npm run build && npm run minify",
		"build:min:start": "npm run build && npm run minify && npm run start",
		"b": "npm run build",
		"bs": "npm run build:start",
		"bl": "npm run build:live",
		"bm": "npm run build:min",
		"bms": "npm run build:min:start",
		"_bm_s": "npm run _preinstall && npm run bm && npm run _postinstall && npm run start",
		"_bms_": "npm run _preinstall && npm run bms && npm run _postinstall",
		"doc": "typedoc --theme default --hideGenerator --out doc/ src/index.ts",
		"lint": "tslint \"./src/**/*.{ts,js}\" --config ./tslint.json -p . --fix --force",
		"prettify": "prettier --config ./.prettierrc.json --write \"./{src,types}/**/*.ts\"",
		"prettify:lint": "npm run prettify && npm run lint",
		"pl": "npm run prettify:lint",
		"test": "mocha -r ts-node/register --timeout 999999 --colors src/**/*.spec.ts",
		"test:debug": "node --inspect-brk=9222 ./node_modules/mocha/bin/_mocha -r ts-node/register --timeout 999999 --colors src/**/*.spec.ts"
	},
	"keywords": [],
	"author": "Amir Al-Omari",
	"license": "UNLICENSED",
	"dependencies": {
		"crypto-js": "^3.1.9-1",
		"express": "^4.16.4",
		"fs": "0.0.1-security",
		"jsonwebtoken": "^8.5.0",
		"mongoose": "^5.4.12",
		"node-minify": "^3.6.0",
		"nodemailer": "^5.1.1",
		"path": "^0.12.7",
		"typescript": "^3.3.3",
		"websocket": "^1.0.28"
	},
	"devDependencies": {
		"@types/crypto-js": "^3.1.43",
		"@types/express": "^4.16.1",
		"@types/jsonwebtoken": "^8.3.0",
		"@types/mocha": "^5.2.5",
		"@types/mongoose": "^5.3.15",
		"@types/node": "^10.12.26",
		"@types/nodemailer": "^4.6.5",
		"@types/websocket": "0.0.40",
		"chai": "^4.2.0",
		"chai-http": "^4.2.1",
		"mocha": "^5.2.0",
		"nodemon": "^1.18.10",
		"ts-node": "^7.0.1",
		"typedoc": "^0.13.0"
	}
}
